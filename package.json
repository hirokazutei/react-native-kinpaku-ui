    "@storybook/react-native-server": "^5.2.3",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.1",
    "jest": "^24.1.0",
    "metro-react-native-babel-preset": "0.54.1",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-native": "^0.60.0",
    "react-native-config": "^0.11.7",
